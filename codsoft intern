include<stdio.h>
#include<conio.h>
int add(int,int);
void main()
{
    int num1,num2,res;
    printf("enter num1: ");
    scanf("%d",&num1);
    printf("enter num2: ");
    scanf("%d",&num2);
    res=add(num1,num2);
    printf("the sum is %d",res);
    getch();
}
int add(int num1,int num2)
{
    int ans;
    ans=num1+num2;
    return ans;
}
